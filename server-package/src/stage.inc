hook OnPlayerKeyStateChange(playerid, newkeys, oldkeys) {
	new idd = GetPlayerCar(playerid);
	if(idd == -1) return Y_HOOKS_CONTINUE_RETURN_1;
	if(!(newkeys & KEY_CROUCH)) return Y_HOOKS_CONTINUE_RETURN_1;
	if(posible_Action[playerid] < STAGE || posible_Action[playerid] > STAGE_MAX) return Y_HOOKS_CONTINUE_RETURN_1; 

	new i = posible_Action[playerid] - STAGE;

	PlayerTextDrawHide(playerid, PressTD[0]);
	PlayerTextDrawHide(playerid, PressTD[1]);
	PlayerTextDrawHide(playerid, PressTD[2]);

	SetPlayerCameraPos(playerid, stagePos[0][i][0], stagePos[0][i][1], stagePos[0][i][2]);
	SetPlayerCameraLookAt(playerid, stagePos[1][i][0], stagePos[1][i][1], stagePos[1][i][2]);
	clearChat.playerid(20);
	new const cost[] = {100,225,500,800}; gString[0] = 0;
	loop.s(0..4) format(gString, _, "%s%sStage (+%dkm/h)\t%d pp\n", gString, CarInfo[idd][cStage] > s ? "~g~":"~r~", stageSpeed[s], cost[s]);
	ShowDialogStyleV(playerid, upgrade_stage, "Buy Stage", "Stage", gString, "Foloeste W si S pentru a selecta.");
	return Y_HOOKS_CONTINUE_RETURN_0;
}

VDialog:upgrade_stage(playerid, bool:response, indexid)<focus> {
	if(!response) return SetCameraBehindPlayer(playerid), 0;
	new idd = GetPlayerCar(playerid);

	if(idd == -1) return true;
	gString[0] = 0;
	new const cost[] = {100,225,500,800};

	if(CarInfo[idd][cStage] > indexid) return SCM(playerid, COLOR_LGREEN, "Eroare: Ai deja acest stage pe vehicul!");
	if(PlayerInfo[playerid][pPremiumPoints] < cost[indexid]) return SCM(playerid, COLOR_LGREEN, "Eroare: Nu ai suficiente puncte premium!");
	if(!CarInfo[idd][cStage]) SetTimerEx(#GiveVehicleSpeed, 300, false, #i, playerid);
	CarInfo[idd][cStage] = indexid+1;
	PlayerInfo[playerid][pPremiumPoints] -= cost[indexid];

	update.gQuery("cars", "Stage", CarInfo[idd][cStage], "ID", CarInfo[idd][cID]);
	update.gQuery("users", "PremiumPoints", PlayerInfo[playerid][pPremiumPoints], "id", PlayerInfo[playerid][pSQLID]);
	HideDialogStyleV(playerid);
	SetCameraBehindPlayer(playerid);
	SCMf(playerid, COLOR_GM, ">> {FFFFFF}Felicitari ti-ai upgradat vehiculul la stage #%d! pentru suma de %d premium points", indexid + 1, cost[indexid]);
	return true;
}

redist::GiveVehicleSpeed(i) {
    if(GetPlayerState(i) == PLAYER_STATE_DRIVER) SetTimerEx(#GiveVehicleSpeed, 300, false, #i, i);
    
    new
        vehicleid = GetPlayerVehicleID(i),
        spe = GetVehicleSpeed(vehicleid),
        keys,
        Float:vx,
        Float:vy,
        Float:vz,
        carid = FindSpawnID(vehicleid)
    ;   

    GetPlayerKeys(i, keys, _:vx, _:vx);

    if(carid == -1) return true;

    if ((keys & (0b001000 | 0b100000 | KEY_HANDBRAKE)) == 0b001000) {
    	GetVehicleVelocity(vehicleid, vx, vy, vz);
		if(spe>GetVehicleMaxSpeed(CarInfo[carid][cModel])+stageSpeed[CarInfo[carid][cStage]-1])
			return true;

	    if (vx * vx + vy * vy < 0.4 * 0.4) return true;


	    vx *= 1.025;
	    vy *= 1.025;


	    if (vz > 0.04 || vz < -0.04)
	        vz -= 0.020;

	    SetVehicleVelocity(vehicleid, vx, vy, vz);
	}
    return true;
}

